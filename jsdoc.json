{
  "source": {
    "include": [
      "src"
    ],
    "exclude": [
      ".git",
      "node_modules"
    ],
    "includePattern": ".+\\.[j|t]s(x|doc)?$"
  },
  "plugins": ["plugins/markdown", "jsdoc-plugin-typescript", "node_modules/jsdoc-babel"],
  "babel": {
    "extensions": ["js", "es6", "jsx", "ts", "tsx"]
  },
  "typescript": {
    "moduleRoot": "src"
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": true
    }
  }
}