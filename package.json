{
  "name": "react-asterisk-table",
  "version": "2.0.11",
  "description": "react-asterisk-table React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "umd"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:ts && find lib/ -name '*.tsx' -delete",
    "build:js": "nwb build-react-component --copy-files",
    "build:ts": "tsc && tsc --project ./tsconfig.build.json",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "./node_modules/jsdoc/jsdoc.js -c ./jsdoc.json",
    "docs:show": "open out/index.html"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/uniqid": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-plugin-typescript": "^2.0.6",
    "lodash": "^4.17.20",
    "nwb": "^0.25.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1",
    "redux": "^4.0.5",
    "source-map-loader": "^1.1.3",
    "ts-loader": "^8.0.12",
    "typescript": "^4.2.2",
    "uniqid": "^5.2.0"
  },
  "author": "Teemu Orava",
  "homepage": "",
  "license": "MIT",
  "repository": "https://github.com/torava/react-asterisk-table",
  "keywords": [
    "react-component react table"
  ]
}
